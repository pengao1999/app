    "chaoyuezu-dev:h5": "cp -f ./src/pages.chaoyuezu.json ./src/pages.json && cross-env NODE_ENV=development UNI_PLATFORM=h5 vue-cli-service uni-serve --mode chaoyuezudev",
    "build:h5": "cross-env NODE_ENV=production UNI_PLATFORM=h5 vue-cli-service uni-build",
